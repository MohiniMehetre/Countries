  target 'CountriesTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya', '~> 13.0'
    pod 'MBProgressHUD', '~> 1.1.0'
    pod 'SVGKit'
    pod 'Fabric'
    pod 'Crashlytics'
  end

  target 'CountriesUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya', '~> 13.0'
    pod 'MBProgressHUD', '~> 1.1.0'
    pod 'SVGKit'
    pod 'Fabric'
    pod 'Crashlytics'
  end

end
